{
  "apps": [
    {
      "name": "sikeskoja",
      "script": "dist/server.js",
      "cwd": "/var/www/sikeskoja",
      "instances": 2,
      "exec_mode": "cluster",
      "env": {
        "NODE_ENV": "production"
      },
      "error_file": "/var/www/sikeskoja/logs/error.log",
      "out_file": "/var/www/sikeskoja/logs/access.log",
      "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
      "merge_logs": true,
      "autorestart": true,
      "max_restarts": 10,
      "min_uptime": "10s"
    }
  ]
}
